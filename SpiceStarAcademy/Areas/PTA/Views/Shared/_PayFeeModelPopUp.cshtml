@model SJModel.PTAModel.PTAFeePaymentViewModel
<body>
    <section>

        <div class="row">
            <div class="col-md-3">
                <div class="form-group">
                    <label for="receiptNumber">Receipt Number</label>
                    @Html.TextBoxFor(m=>m.RecieptNo,new { @class = "form-control", placeholder = "Reciept No", disabled = "disabled" })
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label for="txtapplicationNumber">Application Number</label>
                    @Html.TextBoxFor(m => m.ApplicationNo, new { @class = "form-control", placeholder = "Reciept No", disabled = "disabled" })
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">

                    <label for="txtSession">Session</label>
                    @Html.TextBoxFor(m => m.SessionName, new { @class = "form-control", @disabled = "disabled" })
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label for="txtDate">Date</label>
                    <input type="date" class="form-control" id="txtDate" value="01/10/2019" name="txtDate" disabled="">
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label for="txtName">Name</label>
                    @Html.TextBoxFor(m => m.CandidateName, new { @class = "form-control", @disabled = "disabled" })
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label for="txtGender">Gender</label>
                    @Html.TextBoxFor(m => m.Gender, new { @class = "form-control", @disabled = "disabled" })
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label for="txtEmail">Email</label>
                    @Html.TextBoxFor(m => m.Email, new { @class = "form-control", placeholder = "Name", disabled = "disabled" })
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label for="txtCourse">Course</label>
                    @Html.TextBoxFor(m=>m.CourseName, new { @class = "form-control", @disabled = "disabled" })
                    @Html.TextBoxFor(m=>m.CourseName, new { @class = "form-control", @disabled = "disabled" })

                </div>
            </div>
        </div>

    </section>
    <section>
        <div class="row">
            <div class="col-md-6">
                <div class="row">
                    <div class="col-md-12">
                        <hr style="margin-top: 11px;">
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="row">
                    <div class="col-md-12">
                        <hr style="margin-top: 11px;">
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section>
        <div class="row">
            <div class="col-md-3">
                <div class="form-group">
                    <label for="">Scholarship Type</label>
                    <!-- <label class="lbldisable">Cat-1<i class="fa fa-caret-down"></i></label> -->
                    <select id="txtScholarshipType" class="form-control">
                        <option value="Cat-1">Cat-1</option>
                        <option value="Cat-2">Cat-2</option>
                        <option value="General">General</option>
                    </select>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label for="txtAmount">Amount (INR)</label>
                    <input type="text" class="form-control text-right" id="txtAmount" name="txtAmount" value="11,80,000" disabled="">
                    <!-- <label class="lbldisable amt">11,80,000 INR</label> -->
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label for="txtPaymentType">Payment Type</label>
                    <select class="form-control">
                        <option>Partwise</option>
                        <option>One-time</option>
                    </select>
                </div>
                <!-- <label class="lbldisable">Partwise<i class="fa fa-caret-down"></i></label> -->
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label>Mode of Payment</label>
                    <select class="form-control">
                        <option>DD</option>
                        <option>RTGS</option>
                    </select>
                    <!-- <h6>Mode of Payment:</h6> -->
                    <!-- <label class="lbldisable">DD<i class="fa fa-caret-down"></i></label> -->
                </div>
            </div>
        </div>
        <div class="col-md-12 text-center" style="padding-top:12px">
            <div class="form-group">
                <input type="button" style="width: 70px;" class="btn btn-success" id="btnSubmit" name="btnSubmit" value="Pay">
                <!-- <button>Pay</button> -->
            </div>
        </div>
    </section>

    <section>
        <div class="row">
            <div class="col-md-12">
                <hr>
            </div>
        </div>
    </section>

    <section>
        @*<div class="row">
                <div class="col-md-12">Details of previous payments:</div>
            </div>*@

        <div class="row">
            <div class="col-md-12">
                <div class="mob-scroll">
                    <table id="tblfee" class="table table-striped table-bordered">
                        <caption>Details of previous payments</caption>
                        <thead>
                            <tr>
                                <th class="downloadCss">S.No.</th>
                                <th class="downloadCss">Installment <i class="fas fa-sort-amount-up" class="sortOrder"></i><!-- <i class="fa fa-arrow-down" ></i> --></th>
                                <th class="downloadCss">Payment Type <i class="fas fa-sort-amount-up"></i></th>
                                <th class="downloadCss">Mode of Payment<i class="fas fa-sort-amount-up"></i></th>
                                <th class="downloadCss">Date <i class="fas fa-sort-amount-up"></i></th>
                                <th class="downloadCss">ReferenceID <i class="fas fa-sort-amount-down"></i></th>
                                <th class="downloadCss text-center">Print</th>
                                <th class="downloadCss text-center">Amount (INR)</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>1</td>
                                <td>First</td>
                                <td>One-time</td>
                                <td>RTGS</td>
                                <td>02/06/2019</td>
                                <td>2019RTGS116911</td>
                                <td class="downloadCss"><i class="fa fa-print"></i></td>
                                <td class="text-right">11,80,000/-</td>
                            </tr>
                            <tr>
                                <td>2</td>
                                <td>Second</td>
                                <td>Partwise</td>
                                <td>DD</td>
                                <td>12/01/2020</td>
                                <td>2020DD116911</td>
                                <td class="downloadCss"><i class="fa fa-print"></i></td>
                                <td class="text-right">3,50,000</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </section>
    <script>
        $(function () {
            $('[data-toggle="tooltip"]').tooltip()
            $("#tblfee").DataTable();
        })
        $(function () {
            $('[data-toggle="popover"]').popover()
        })
    </script>
</body>



