@model SJModel.PTAModel.StudentInfoViewModel
@{
    ViewBag.Title = "Student Profile";
    Layout = "~/Areas/PTA/Views/Shared/_Layout.cshtml";
}
<style>
    .alert1 {
        padding: 0px;
        margin-bottom: 10px;
        border-radius: 4px;
    }

    .select2-container--default .select2-selection--single {
        border-radius: 0px !important;
    }

    .select2-container .select2-selection--single {
        height: 34px !important;
    }

    .fee-stucture-title {
        font-size: 25px;
        font-weight: bold;
    }

    .payment-done {
        color: green;
        font-weight: bold;
    }

    .payment-pending {
        color: red;
        font-weight: bold;
    }

    .done-section {
        background-color: #b9f9b9;
        font-weight: bold;
    }

    .title-fee {
        font-weight: 600;
        font-size: 15px;
    }

    .fee-heading {
        background-color: #666 !important;
        color: #fff;
    }

    .installment-title {
        font-weight: bold;
    }
</style>
<link href="~/Plugins/iCheck/square/blue.css" rel="stylesheet" />

<div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog" style="width:1024px !important; ">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="col-md-12 text-center">Admission Fee Payment</h4>
            </div>
            <div class="modal-body">
                <div id="payFees"></div>
                @* @Html.Partial("_PayFeeModelPopUp")*@
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Student Profile
            <button type="button" onclick="OpenFeePopModel('@(Model.ApplicationNo)')" class="btn btn-success pull-right">Pay Fees</button>
        </h1>
    </section>
    <!-- Main content -->
    <section class="content">

        <div class="row">
            <div class="col-md-3">
                <!-- Profile Image -->
                <div class="box box-primary">
                    <div class="box-body box-profile">
                        <img class="profile-user-img img-responsive img-circle" src="/Images/profile.jpg" id="imgProfile" alt="User profile picture">
                        <h3 class="profile-username text-center">@(Model.CandidateName)</h3>
                        <p class="text-muted text-center"> @Model.Email </p>
                        <div class="row">
                            <div class="col-md-12">
                                <input type="file" id="candidateuploadFile" style="width:100%" class="form-control" />
                            </div>
                        </div>
                        <div class="row" style="padding-left:65px;padding-top:15px;">
                            <div class="col-md-12">
                                <input type="button" class="btn btn-success" id="btncandidateUpload" value="Upload" />
                                <input type="button" class="btn btn-danger" id="btncandidateRemove" onclick="removeimage('@(Model.ApplicationNo)')" value="Remove" />
                            </div>
                        </div>
                    </div>
                    <!-- /.box-body -->
                </div>
            </div>
            <div class="col-md-9">
                <div class="alert alert-success fade in alert-dismissable text-center" id="dvMsg" style="display:none;">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                    <strong id="lblMsg">@TempData["msg"]</strong>
                </div>
                <div class="nav-tabs-custom">
                    <ul class="nav nav-tabs">
                        <li class="active"><a href="#basicDetail" data-toggle="tab">Basic Detail</a></li>
                        <li><a href="#medicalDetail" data-toggle="tab">Medical Detail</a></li>
                        <li><a href="#addressDetail" data-toggle="tab">Address Detail</a></li>
                        <li><a href="#FeeDetail" data-toggle="tab">Fee Detail</a></li>
                        <li><a href="#educationDetail" data-toggle="tab">Education Detail</a></li>
                        <li><a href="#docsDetails" data-toggle="tab">Upload Docs</a></li>
                    </ul>
                    <div class="tab-content">
                        <div class="active tab-pane" id="basicDetail">
                            <form class="form-horizontal" id="frmBasicDetail" onsubmit="return validateBasicDetail();" action="/StudentProfile/UpdateProfileAllInfo" method="post">
                                <div class="form-group col-md-6">
                                    <label for="inputName" class="col-sm-4 control-label">Application No</label>
                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(x => x.ApplicationNo, new { @class = "form-control", @disabled = "disabled" })
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="inputName" class="col-sm-4 control-label">Session</label>
                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(x => x.SessionName, new { @class = "form-control", @disabled = "disabled" })
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="inputName" class="col-sm-4 control-label">Course</label>
                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(x => x.CourseName, new { @class = "form-control", @disabled = "disabled" })
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="inputSkills" class="col-sm-4 control-label">Batch Name</label>
                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(x => x.Batch, new { @class = "form-control", @disabled = "disabled" })
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="inputName" class="col-sm-4 control-label">Email</label>
                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="inputExperience" class="col-sm-4 control-label">DOB</label>
                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(x => x.DOB, new { @class = "form-control", autocomplete = "off", onkeydown = "return false;" })
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="inputSkills" class="col-sm-4 control-label">Mobile No.</label>
                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(x => x.Mobile, new { @class = "form-control", onkeypress = "return isNumber(event,this)", @maxlength = "12" })
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="inputSkills" class="col-sm-4 control-label">Education</label>
                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(x => x.Education, new { @class = "form-control", @disabled = "disabled" })
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="inputSkills" class="col-sm-4 control-label">Valid Passport</label>
                                    <div class="col-sm-8">
                                        @Html.CheckBoxFor(x => x.ValidPassport)
                                    </div>
                                </div>
                                <div class="form-group" style="padding-left:75px; margin-left:35px">
                                    <div class="col-sm-offset-1 col-sm-10">
                                        <button type="button" onclick="SaveBasicsDetails();" class="btn btn-success">Update</button>
                                        <button type="button" onclick="window.location.href = '/Addmission/AddmissionView'" class="btn btn-default">Cancel</button>
                                    </div>
                                </div>
                            </form>
                        </div>

                        <div class="tab-pane" id="addressDetail">
                            @Html.HiddenFor(m => m.AddressInfo.Id)
                            <form class="form-horizontal" id="frmBasicDetail">

                                <div class="form-group">
                                    <label for="inputName" class="col-sm-3 control-label">Correspondence Address 1</label>
                                    <div class="col-sm-3">
                                        @Html.TextBoxFor(x => x.AddressInfo.CorAddress1, new { @class = "form-control", placeholder = "correspondence Address" })
                                    </div>
                                    <div>
                                        <label for="inputName" class="col-sm-3 control-label">Correspondence Address 2</label>
                                        <div class="col-sm-3">
                                            @Html.TextBoxFor(x => x.AddressInfo.CorAddress2, new { @class = "form-control", placeholder = "correspondence Address" })
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputName" class="col-sm-3 control-label">Correspondence Country</label>
                                    <div class="col-sm-3">
                                        @Html.DropDownList("ddlcorCountry", new SelectList(Model.CountryList, "Id", "Name", Model.AddressInfo.CorCountry), "Select Country", new { @class = "form-control", onchange = "fillcitycountry('State')", placeholder = "correspondence Country" })
                                    </div>

                                    <div>
                                        <label for="inputName" class="col-sm-3 control-label">Correspondence State</label>
                                        <div class="col-sm-3">
                                            <select id="ddlcorState" onchange="fillcitycountry('District')" class="form-control">
                                                <option value="">Select State</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputEmail" class="col-sm-3 control-label">Correspondence District</label>
                                    <div class="col-sm-3">
                                        <select id="ddlCordistrict" onchange="fillcitycountry('City')" class="form-control">
                                            <option value="">Select District</option>
                                        </select>
                                    </div>
                                    <label for="inputEmail" class="col-sm-3 control-label">Correspondence City</label>
                                    <div class="col-sm-3">
                                        <select id="ddlcorCity" class="form-control">
                                            <option value="">Select City</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputExperience" class="col-sm-3 control-label">Correspondence Zip</label>
                                    <div class="col-sm-3">
                                        @Html.TextBoxFor(x => x.AddressInfo.CorPin, new { @class = "form-control", placeholder = "correspondence Zip", onkeypress = "return isNumber(event,this)", @maxlength = "6" })
                                    </div>
                                </div>
                                <div class="form-group" style="padding-top:10px;padding-bottom:10px;">
                                    <div class="col-sm-offset-3 col-sm-9">
                                        <div class="checkbox">
                                            <label>
                                                @Html.CheckBox("chkAddres", new { onclick = "fillPermanentAdd(this)" })  Same as correspondence
                                            </label>
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="inputName" class="col-sm-3 control-label">Permanent Address 1</label>
                                    <div class="col-sm-3">
                                        @Html.TextBoxFor(x => x.AddressInfo.PerAddress1, new { @class = "form-control", placeholder = "Permanent Address1" })
                                    </div>
                                    <div>
                                        <label for="inputName" class="col-sm-3 control-label">Permanent Address 2</label>
                                        <div class="col-sm-3">
                                            @Html.TextBoxFor(x => x.AddressInfo.PerAddress2, new { @class = "form-control", placeholder = "Permanent Address 2" })
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputName" class="col-sm-3 control-label">Permanent Country</label>
                                    <div class="col-sm-3">
                                        @Html.DropDownList("ddlPerCountry", new SelectList(Model.CountryList, "Id", "Name", Model.AddressInfo.PerCountry), "Select Country", new { @class = "form-control", onchange = "fillPercitycountry('State')", placeholder = "Permanent Country" })
                                    </div>
                                    <div>
                                        <label for="inputName" class="col-sm-3 control-label">Permanent State</label>
                                        <div class="col-sm-3">
                                            <select id="ddlPerState" onchange="fillPercitycountry('District')" class="form-control">
                                                <option value="">Select State</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputEmail" class="col-sm-3 control-label">Permanent District</label>
                                    <div class="col-sm-3">
                                        <select id="ddlPerdistrict" onchange="fillPercitycountry('City')" class="form-control">
                                            <option value="">Select District</option>
                                        </select>
                                    </div>
                                    <div>
                                        <label for="inputEmail" class="col-sm-3 control-label">Permanent City</label>
                                        <div class="col-sm-3">
                                            <select id="ddlPerCity" class="form-control">
                                                <option value="">Select City</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputEmail" class="col-sm-3 control-label">Permanent Zip</label>
                                    <div class="col-sm-3">
                                        @Html.TextBoxFor(x => x.AddressInfo.PerPin, new { @class = "form-control", placeholder = "Permanent Zip", onkeypress = "return isNumber(event,this)", @maxlength = "6" })
                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="col-sm-offset-3 col-sm-9">
                                        <button type="button" onclick="UpdateAddressDetail();" class="btn btn-success">Update</button>
                                        <button type="button" onclick="window.location.href = '/Addmission/AddmissionView'" class="btn btn-default">Cancel</button>
                                    </div>
                                </div>
                            </form>
                        </div>

                        <div class="tab-pane" style="padding:5px 25px 25px" id="educationDetail">
                            <!-- <h3>Educational Qualifications</h3> -->
                            <form>

                                @foreach (var item in Model.EducationInfo)
                                {
                                    if (item.Class != "UG")
                                    {
                                        <div class="page-header">
                                            <h3>@item.Class Class</h3>
                                        </div>
                                        <div class="row grid-divider">
                                            <div class="col-sm-3">
                                                <div class="col-padding">
                                                    <h4>Name Of Institute</h4>
                                                    <input type="text" id="NameOfInstitute@(item.Class)" value="@item.NameOfInstitute" class="form-control" placeholder="Institute Name" />
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="col-padding">
                                                    <h4>Board/University</h4>
                                                    <input type="hidden" id="hdn@(item.Class)" value="@item.ptaBoardAndUniversityMasterId" />
                                                    @Html.DropDownList("ddlboardandun" + item.Class, new SelectList(Model.GetBoardAndUniversityList, "Id", "Name"), "Please Select", new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="col-padding">
                                                    <h4>Year Of Passing</h4>
                                                    <input type="text" id="YearOfPassing10th" class="form-control" value="@item.YearOfPassing" placeholder="Year of Passing" />
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="col-padding">
                                                    <h4>Evaluation Type</h4>
                                                    <input type="hidden" id="hdn@(item.Class)" value="@item.ptaEduEvaluationTypeMasterId" />
                                                    @Html.DropDownList("ddlEduEvu" + item.Class, new SelectList(Model.GetEvaluationTypeList, "Id", "Name"), "Please Select", new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="col-padding">
                                                    <h4>Percentage/CGPA</h4>
                                                    <input type="text" id="PercentageOrCGPA10th" class="form-control" value="@item.PercentageOrCGPA" placeholder="Percentage" />
                                                </div>
                                            </div>
                                        </div>


                                        if (item.SubjectMarks.Count > 0)
                                        {
                                            <div class="row grid-divider">
                                                <div class="col-sm-12"><h4 style="background-color: #e4e4e4; padding: 3px;">Marks scored</h4></div>
                                            </div>
                                            <div class="row grid-divider">
                                                <div class="col-sm-3">
                                                    <div class="col-padding">
                                                        <h4>Subjects</h4>
                                                    </div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div class="col-padding">
                                                        <h4>Maximum Marks</h4>
                                                        <p></p>
                                                    </div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div class="col-padding">
                                                        <h4>Marks obtained</h4>
                                                        <p></p>
                                                    </div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div class="col-padding">
                                                        <h4>Percentage</h4>
                                                        <p></p>
                                                    </div>
                                                </div>
                                                @foreach (var obj in item.SubjectMarks)
                                                {
                                                    <div class="col-sm-3">
                                                        <div class="col-padding">
                                                            <h4> @obj.SubjectName </h4>
                                                            <p></p>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="col-padding">
                                                            <h4></h4>
                                                            <input type="text" id="" class="form-control" value="@obj.MaximumMarks" placeholder="Percentage" />
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="col-padding">
                                                            <h4></h4>
                                                            <input type="text" id="" class="form-control" value="@obj.ObtainedMarks" placeholder="Percentage" />
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="col-padding">
                                                            <h4></h4>
                                                            <input type="text" id="" class="form-control" value="@obj.Percentage" placeholder="Percentage" />
                                                        </div>
                                                    </div>
                                                }

                                            </div>
                                        }
                                    }
                                    else
                                    {
                                        <div class="page-header">
                                            <h3>@item.Class</h3>
                                        </div>
                                        <div class="row grid-divider">
                                            <div class="col-sm-3">
                                                <div class="col-padding">
                                                    <h4>Name Of Institute</h4>
                                                    <input type="text" id="NameOfInstituteGraguate" value="@item.NameOfInstitute" class="form-control" placeholder="Name of Institute" />
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="col-padding">
                                                    <h4>Board/University</h4>
                                                    <input type="hidden" id="hdn@(item.Class)" value="@item.ptaUGBoardAndUniversityMasterId" />
                                                    @Html.DropDownList("UGddlboardandun" + item.Class, new SelectList(Model.GetBoardAndUniversityListUG, "Id", "Name"), "Please Select", new { @class = "form-control" })
                                                    @*@Html.DropDownListFor(x => x.GetBoardAndUniversityListUG, new SelectList(Model.GetBoardAndUniversityListUG, "Id", "Name"), "Please Select", new { @class = "form-control" })*@
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="col-padding">
                                                    <h4>Degree</h4>
                                                    <p><input type="text" id="degreeUG" name="txtDegreeUG" class="form-control"></p>
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="col-padding">
                                                    <h4>Year Of Passing</h4>
                                                    <input type="text" id="YearofPassingundergraduate" value="@item.YearOfPassing" class="form-control" placeholder="Year of Passing" />
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row grid-divider">
                                            <div class="col-sm-3">
                                                <div class="col-padding">
                                                    <h4>Result Status</h4>
                                                    @Html.DropDownListFor(x => x.Resultstatusinfo, new SelectList(Model.Resultstatusinfo, "Id", "Name"), "Please Select", new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="col-padding">
                                                    <h4>Evaluation Type</h4>
                                                    <input type="hidden" id="hdn@(item.Class)" value="@item.ptaEduEvaluationTypeMasterId" />
                                                    @Html.DropDownList("ddlEduEvu" + item.Class, new SelectList(Model.GetEvaluationTypeList, "Id", "Name"), "Please Select", new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="col-padding">
                                                    <h4>Percentage/CGPA</h4>
                                                    <input type="text" id="PercentageOrCGPAgraduate" class="form-control" placeholder="Percentage" />
                                                </div>
                                            </div>
                                        </div>
                                    }
                                }
                            </form>
                        </div>


                        @**********************Fee Details**************************@
                        <div class="tab-pane" style="padding:5px 25px 25px" id="FeeDetail">
                            <form action="/StudentProfile/AddFeeType" method="post" onsubmit="return validate();">
                                <div class="row" style="pointer-events: all;">
                                    <div class="col-md-4" style="pointer-events: all;">
                                        <div class="form-group" style="pointer-events: all;">
                                            <label>Course Name:</label>
                                            @Html.DropDownListFor(m => m.CourseId, new SelectList(Model.CourseList, "Id", "Name"), "Select Course Name", new { @class = "form-control" })
                                        </div>
                                    </div>

                                    <div class="col-md-4" style="pointer-events: all;">
                                        @Html.HiddenFor(m => m.Id)
                                        <div class="form-group has-feedback" style="pointer-events: all;">
                                            <label>Session:</label>
                                            <select name="SessionMasterId" id="SessionMasterId" class="form-control">
                                                <option value=""> Select Session </option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-4" style="pointer-events: all;">
                                        <div class="form-group" style="pointer-events: all;">
                                            <label>Fee Name:</label>
                                            @Html.DropDownListFor(m => m.FeeTypeId, new SelectList(Model.FeeTypeList, "Id", "Name"), "Select Fee Type", new { @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="row" style="pointer-events: all;">
                                    <div class="col-md-4" style="pointer-events: all;">
                                        <div class="form-group has-feedback" style="pointer-events: all;">
                                            <label>Amount:</label>
                                            @Html.TextBoxFor(m => m.Amount, new { @class = "form-control", onkeypress = "return isDecimalNumber(event,this)", placeholder = "Amount" })
                                        </div>
                                    </div>

                                    <div class="col-md-4" style="pointer-events: all;">
                                        <div class="form-group has-feedback" style="pointer-events: all;">
                                            <label>Active:</label><br />
                                            @Html.CheckBoxFor(m => m.IsActive)
                                        </div>
                                    </div>
                                </div>
                                <br>

                                <div class="row text-center" style="pointer-events: all;">
                                    <div class="col-md-12" style="pointer-events: all;">
                                        <div class="form-group pull-left" style="pointer-events: all;">
                                            <button type="submit" id="btnSubmit" class="btn btn-success"> Submit  </button> &nbsp;
                                            <a class="btn btn-default" id="btnFormClear" href="JavaScript:Void(0);">Clear</a>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>

                        @********************Documents Upload**************************@

                        <div class="tab-pane" id="docsDetails">
                            <div class="row grid-divider">
                                <div class="col-md-8">
                                    <div id="divmedicalmodel">
                                    </div>
                                </div>
                            </div>
                            <hr />
                            <div class="row grid-divider">
                                <div class="col-md-4">
                                    <input type="file" id="educationUploadFile" style="width:100%" class="form-control" />
                                </div>
                                <div>
                                    <input type="button" class="btn btn-success" id="btnEducationUpload" value="Upload" />
                                </div>
                                <div class="col-md-12">

                                    <table class="table-responsive table table-striped" id="tblshowuploadfiles" border="1" style="width:100%;font-weight:600">
                                        <thead>
                                            <tr>
                                                <th>S.No</th>
                                                <th>Document Title</th>
                                                <th>Type</th>
                                                <th>Date of Upload</th>
                                                <th>Download</th>
                                                <th>Action</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            @{int i = 0;}
                                            @foreach (var item in Model.ptaDocumentDetails)
