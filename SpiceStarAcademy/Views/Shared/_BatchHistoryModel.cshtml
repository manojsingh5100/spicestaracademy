@model List<SJModel.RoleViewModel>
@{
    int num = 1;
}
<div class="row">
    <div class="col-xs-12">
        <div class="box box-primary">
            <!-- /.box-header -->
            <div class="box-body table-responsive no-padding">
                <table class="table table-hover">
                    <tbody>
                        @if (Model.Count > 0)
                        {
                            <tr>
                                <th>Sr.No</th>
                                <th>Batch From</th>
                                <th>Batch To</th>
                                <th>Description</th>
                            </tr>
                            foreach (var item in Model)
                            {
                                <tr>
                                    <td>@num</td>
                                    <td>@item.ActiveStr</td>
                                    <td>@item.Name</td>
                                    <td>@item.Description</td>
                                </tr>
                                num = num + 1;
                            }
                        }
                    </tbody>
                </table>
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>
</div>

