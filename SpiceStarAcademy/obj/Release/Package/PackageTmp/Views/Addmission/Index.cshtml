@{
    ViewBag.Title = "Admission";
}
<style>
    input[type=search] {
        height: 30px;
        -webkit-appearance: searchfield;
    }
</style>
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Admission
        </h1>
        <ol class="breadcrumb">
            <li class="active"><i class="fa fa-dashboard"></i> &nbsp; Admission</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-xs-12">
                <!-- /.box -->
                <div class="box">
                    <div class="box-body">
                        <div class="row col-md-12 table-responsive">
                            <table id="tblAddmission" class="table table-bordered table-striped" style="width:100%">
                                <thead>
                                    <tr>
                                        <th>Sno</th>
                                        <th>Reg.No</th>
                                        <th>Name</th>
                                        <th>Email</th>
                                        <th>Mobile</th>
                                        <th>DOB</th>
                                        <th>Gender</th>
                                        <th>Course Name</th>
                                        <th style="width:100px">Action</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </section>
    <!-- /.content -->
</div>
<script>
    $(document).ready(function () {
        TableCourseFilter = 7;
        Table = $("#tblAddmission").DataTable(admissiontblSetting("tblAddmission", "GetAddmissionList", "Addmission", 1));
        $('#tblAddmission_length').css("display", "none");
        $.fn.dataTable.ext.errMode = 'throw';
    });
</script>




