
@{
    ViewBag.Title = "CandidateTermination";
}
<title>Candidate Termination/Resignation</title>
<div class="content-wrapper">
    <section class="content-header">
        <h1>Candidate Termination/Resignation</h1>
        <ol class="breadcrumb">
            <li class="active"><i class="fa fa-dashboard"></i> &nbsp;Termination/Resignation</li>
        </ol>
    </section>
    <section class="content">
        <div class="row">
            <div class="col-xs-12">
                <div class="box">
                    <div class="box-body">
                        <div class="row col-md-12 table-responsive">
                            <table id="tblCandidateTermination" class="table table-bordered table-striped" style="width:100%">
                                <thead>
                                    <tr>
                                        <th>Sr.No</th>
                                        <th>Reg.No</th>
                                        <th>Student Name</th>
                                        <th>EmailID</th>
                                        <th>Mobile No</th>
                                        <th>DOB</th>
                                        <th>Gender</th>
                                        <th>Payment Status</th>
                                        <th>Course</th>
                                        @*<th>Batch Name</th>*@
                                        <th>Reg.Date</th>
                                        <th>Action</th>
                                        @*<th>Remark</th>*@
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>
<script>
    $(document).ready(function () {
        TableCourseFilter = 9;
        Table = $("#tblRejectRegistration").DataTable(rejectedtblSetting("tblRejectRegistration", "RejectedCandidates", "Registration", 1));
        $.fn.dataTable.ext.errMode = 'throw';
        $('.buttons-excel').css('display', 'none');
        $('.dt-buttons').append("<img src='/Images/excel-icon-transparent-18.jpg' height=32px; width=60px; onclick='btnexport();' title='Export' style='border:1px solid;border-color:#f5eeee;cursor: pointer;' />");
    });
</script>

