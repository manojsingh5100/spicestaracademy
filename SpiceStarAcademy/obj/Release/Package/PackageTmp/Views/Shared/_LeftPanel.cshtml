<aside class="main-sidebar">
    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">
        <!-- Sidebar user panel -->
        <div class="user-panel">
            <div class="pull-left image">
                <img src="~/assets/img/user2-160x160.png" class="img-circle" alt="User Image">
            </div>
            <div class="pull-left info">
                <p>@Session["UserName"]</p>
                <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
            </div>
        </div>
        <!-- sidebar menu: : style can be found in sidebar.less -->
        <ul class="sidebar-menu" data-widget="tree">
            <li class="header">MAIN NAVIGATION</li>
            @if (Session["RoleName"] != null && Convert.ToString(Session["RoleName"]) != "Reservations")
            {
                <li>
                    <a href="/DashBoard" onclick="Loading();">
                        <i class="fa fa-dashboard"></i> <span>DashBoard</span>
                    </a>
                </li>

                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-address-book"></i>
                        <span>Registration</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        @*<li><a href="/Registration/Create" onclick="Loading();"><i class="fa fa-circle-o"></i>New Registration</a></li>*@
                        <li><a href="/Registration" onclick="Loading();"><i class="fa fa-circle-o"></i>View Registration</a></li>
                    </ul>
                </li>

                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-search-minus"></i>
                        <span>Screening</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="/Registration/ScreenningClearance" onclick="Loading();"><i class="fa fa-circle-o"></i> View Screening</a></li>
                    </ul>
                </li>
                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-plus-square"></i>
                        <span>Medical</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="/Registration/MedicalClearance" onclick="Loading();"><i class="fa fa-circle-o"></i>Medicals and payment</a></li>
                    </ul>
                </li>
                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-superpowers"></i>
                        <span>Admission</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        @*<li><a href="/Addmission"><i class="fa fa-circle-o" onclick="Loading();"></i> Admission</a></li>*@
                        <li><a href="/Addmission/AddmissionView"><i class="fa fa-circle-o" onclick="Loading();"></i> View Admission</a></li>
                    </ul>
                </li>

                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-money"></i>
                        <span>Fee Management</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        @*<li><a href="/FeeManagement/AddFeeType" onclick="Loading();"><i class="fa fa-circle-o"></i>Add FeeType</a></li>*@
                        <li><a href="/FeeManagement/FeeCandidatesList" onclick="Loading();"><i class="fa fa-circle-o"></i>Fee Candidate List</a></li>
                        @*<li><a href="/FeeManagement/AddFeeCollection" onclick="Loading();"><i class="fa fa-circle-o"></i>Add Fee Collection</a></li>*@
                    </ul>
                </li>

                <li style="display:none">
                    <a href="/FeeManagement/FeeRefund" onclick="Loading();">
                        <i class="fa fa-repeat"></i> <span>Fee Refund</span>
                    </a>
                </li>

                <li>
                    <a href="/Registration/WithdrawalCandidates" onclick="Loading();">
                        <i class="fa fa-recycle"></i> <span>Withdrawal Candidates</span>
                    </a>
                </li>

                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-book"></i>
                        <span>Report</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="/Report/Index" onclick="Loading();"><i class="fa fa-circle-o"></i>Report</a></li>
                        <li><a href="/Report/SSAReport" onclick="Loading();"><i class="fa fa-circle-o"></i>SSA Report</a></li>
                        <li><a href="/Report/RevenueReport" onclick="Loading();"><i class="fa fa-circle-o"></i>Revenue Report</a></li>
                    </ul>
                </li>

                if (Session["RoleName"] != null && Session["RoleName"].ToString() == "Admin")
                {
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-shield"></i>
                            <span>Security</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/Login/UserRegistration" onclick="Loading();"><i class="fa fa-circle-o"></i>Create User</a></li>
                        </ul>
                    </li>


                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-list"></i>
                            <span>Control Panel</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/FeeManagement/AddFeeType" onclick="Loading();"><i class="fa fa-circle-o"></i>Add Fee Type</a></li>
                            <li><a href="/Admin" onclick="Loading();"><i class="fa fa-circle-o"></i> <span>Control Panel</span></a></li>
                        </ul>
                    </li>

                    @*<li>
                        <a href="/Admin" onclick="Loading();"><i class="fa fa-list"></i> <span>Control Panel</span></a>
                    </li>*@
                }
            }
            else
            {
                <li>
                    <a href="/DashBoard" onclick="Loading();">
                        <i class="fa fa-dashboard"></i> <span>DashBoard</span>
                    </a>
                </li>
                <li>
                    <a href="/CallCenterInfo" onclick="Loading();">
                        <i class="fa fa-support"></i> <span>Call Center Info</span>
                    </a>
                </li>
            }
        </ul>
    </section>
    <!-- /.sidebar -->
</aside>

